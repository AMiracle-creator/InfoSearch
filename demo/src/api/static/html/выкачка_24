 fs.signup.start.controllerUrl = '/signup/signup/';
 (function () {

 var counter = {
 init: function () {
 var el = document.querySelector('#user-count');
 var value = 16787110;
 var $cookie = $.cookie('seo_user_count');

 value = $cookie ? parseInt($cookie) : value;

 if (!$cookie) {
 $.cookie('seo_user_count', value, {path: '/' });
 }

 var od = new Odometer({
 el: el,
 value: value,
 duration: 200,
 format: '( ddd)'
 });

 var interval = setInterval(function(){
 value = value + Math.floor(Math.random() * 6) + 1;
 od.update(value);
 $.cookie('seo_user_count', value, {path: '/' })
 }, 3000);
 }
 };

 window.land = {
 lastPage: false,
 init: function () {
 var self = this;

 $('body').addClass('newmain--body');

 self.style();

 $('.signup-sex__button').on('click', function (e) {
 self.toggleToSignupForm();
 var sex = e.target.getAttribute('data-sex');
 $('input[value="' + sex + '"]').attr('checked', true);
 });

 $('.js_signup_bottom_button').on('click', function (e) {
 $("html, body").animate({ scrollTop: 0 }, "fast");
 self.toggleBack();
 });

 $('.js_signup_login_bottom_button').on('click', function (e) {
 $("html, body").animate({ scrollTop: 0 }, "fast");
 self.toggleToLogin();
 return false;
 });



 <script>
 fs.onload(function(){
 function fsResizeWindowEvent(){
 var scrWidth = document.body.clientWidth == window.innerWidth ? window.innerWidth : document.body.clientWidth;
 var scrWidthNeed = document.body.clientWidth == window.innerWidth ? 1441 : 1431;
 var $target = document.querySelector('._js-placement-target-signup');
 var url = '/fast/king/admediator.php?c=51903&b=436573';
 if (scrWidth <= scrWidthNeed) {
 $target.setAttribute('height', 100);
 $target.setAttribute('width', 1000);
 url = '/fast/king/admediator.php?c=51903&b=436574';
 $('.signup_signup-page-target-block').addClass('isLongBlock');
 $('.signup__body').addClass('isLongBlock');
 } else {
 $target.setAttribute('height', 500);
 $target.setAttribute('width', 400);
 url = '/fast/king/admediator.php?c=51903&b=436573'
 $('.signup_signup-page-target-block').removeClass('isLongBlock');
 $('.signup__body').removeClass('isLongBlock');
 }

 $target.src = url;
 };
 fsResizeWindowEvent();
 window.onresize = fsResizeWindowEvent;
 })

 </script>

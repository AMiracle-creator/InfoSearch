 targetTime = $.cookie('fkmsgtime');
 }
 var timeLeft = targetTime - now;
 if (timeLeft > 0) {
 st(function () {
 showCounter();
 }, timeLeft);
 } else {
 showCounter();
 }
 });
 </script>
 </div>

 
 
 
 
 
 
 </div>




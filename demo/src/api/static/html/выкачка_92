<script>
window.PlayerjsEvents = (function () {
var heartbeat = {};
function isLive(playerId) {
return window.location.pathname.includes("bonustv") || playerId.includes("video_online_small");
}
function getMediascopeType(playerId) {
return isLive(playerId) ?
1 : // Live
2; // VOD
}
function getMediascopeMedia(playerId) {
return isLive(playerId) ?
encodeURIComponent('Звезда ТВ') : // Live
encodeURIComponent('Звезда'); // VOD
}
function getCurrentTime(playerId) {
return isLive(playerId) ?
getSeconds(Date.now() / 1000) : // Live
getSeconds(document.querySelector('#' + playerId + ' video').currentTime); // VOD
}
function getDuration(playerId) {
return getSeconds(document.querySelector('#' + playerId + ' video').duration);
}
function getSeconds(time) {
return time.toString().split('.')[0];
}
function startHeartbeat(playerId, event) {
heartbeat[playerId] = setInterval(function () {
sendMediascopeEvent(playerId, event);
}, 30000);
}
function stopHeartbeat(playerId) {
clearInterval(heartbeat[playerId]);
}
function sendMediascopeEvent(playerId, eventId, isVast, time) {
sendMediascopeEvent.send(
(isVast ? 'adv_' : '') + window.location.pathname,
eventId,
getMediascopeType(playerId),
getMediascopeMedia(playerId),
time || getCurrentTime(playerId)
);
}
sendMediascopeEvent.send = function (uid, view, type, media, currentTime) {
const url = 'https://tns-counter.ru/e/ec01&cid=zvezda_tv&typ=1&tms=zvezda_tv&idc=166&idlc=' + uid + '&urlc=' + encodeURIComponent(location.origin + location.pathname) + '&ver=0&media=' + media + '&view=' + view + '&type=' + type + '&fts=' + currentTime;
return fetch(url, {method: 'GET', credentials: 'include'});
};
return function (event, id, info) {
if (event === 'play' || event === 'vast_start') {
$(".js-showhide").fadeOut();
}
if (event === 'pause' || event === 'vast_finish') {
$(".js-showhide").fadeIn();
}
if (event === 'pause' && getCurrentTime(id) === getDuration(id)) {
return;
}
const events = {
end: 0,
finish: 0,
stop: 0,
play: 1,
heartbeat: 2,
pause: 3,
vast_complete: 0,
vast_start: 1,
// vast_time: 2,
vast_click: 4,
};
const vast = event.indexOf('vast_') === 0;
if (events.hasOwnProperty(event)) {
sendMediascopeEvent(id, events[event], vast);
if (event === 'play') {
startHeartbeat(id, events.heartbeat);
} else if ([0, 3].includes(events[event])) {
stopHeartbeat(id);
}
}
if (event === 'time') {
var time = getSeconds(info);
setTimeout(function () {
PlayerjsEvents[id] = {
time: time,
};
}, 100);
}
if (event === 'seek') {
sendMediascopeEvent(id, events.stop, false, PlayerjsEvents[id].time);
sendMediascopeEvent(id, events.play, false);
}
}
})();
</script>

<script>




var liveurl = "https://tvchannelstream1.tvzvezda.ru/cdn/tvzvezda/playlist.m3u8";

var urn = window.location.origin.replace("m.tvzvezda","tvzvezda")+window.location.pathname;
var video_online_smallObj = {"url":urn, "embed":urn, m:"video_online_small",id:"video_online_small",file:liveurl, poster:'https://tvchannelstream1.tvzvezda.ru/cdn/tvzvezda/preview.jpg', h:83,  heartbeat:"//www.tns-counter.ru/V13a**catid:21:vcid:258:vcver:0:fts:[fts]:vts:[vts]:evtp:1:dvtp:[dvtp]**zvezda_tv/ru/UTF-8/tmsec=zvezda_zvezda-hb30/", livets:1676908186 };
var playerOnlineSmall = new Playerjs(video_online_smallObj);
function PlayerjsFsButton(){
playerOnlineSmall.api('stop');
$('.bonustv').magnificPopup('open');
}
$(document).ready(function() {
$('.bonustv').magnificPopup({
disableOn: 700,
type: 'iframe',
mainClass: 'mfp-fade',
removalDelay: 160,
preloader: false,
fixedContentPos: true
});





});
</script>


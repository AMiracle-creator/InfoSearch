$(window).scroll(function() {
var css_change_razdel_from = $(".css_change_razdel_from");
if (css_change_razdel_from.length > 0) {
var eTop = css_change_razdel_from.offset().top; //get the offset top of the element
var offset = eTop - $(window).scrollTop();
//console.log(offset);
if (offset < 100) {
css_change_razdel_from.removeClass("show");
css_change_razdel_from.addClass("hide");
} else {
css_change_razdel_from.removeClass("hide");
css_change_razdel_from.addClass("show");
}
}
});

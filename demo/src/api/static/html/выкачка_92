<script>
function createBanner(config) {
window.yaContextCb.push(function() {
window.Ya.adfoxCode.create(config);
});
}
createBanner.adaptive = function() {
const options = arguments;
window.yaContextCb.push(function() {
window.Ya.adfoxCode.createAdaptive.apply(window.Ya.adfoxCode, options);
});
}
</script>

<script>
setTimeout(function() {
if (window.canRunAds === undefined){
ga('send', {
hitType: 'event',
eventCategory: 'session',
eventAction: 'Adblock',
eventLabel: 'true'
});
}
// Тип баннера: nd-tgb-1
if (document.getElementById('adfox_151929244940397755')) {
createBanner({
ownerId: 206858,
containerId: 'adfox_151929244940397755',
params: {
pp: 'g',
ps: 'bhfd',
p2: 'fvek'
}
});
}
function onRender(timeout, container) {
function bindSticky() {
if ($('#' + container).is(':empty')) {
return;
}
var stickyDiv = document.getElementById('sticky'),
stickyClose = document.getElementById('stickyClose'),
billboardYa = document.getElementById('billboardYa'),
rect = stickyDiv.getBoundingClientRect();
billboardYa.style.height = Math.max(rect.height, 90) + 'px';
if (window.location.pathname === '/') {
return;
}
stickyDiv.style.position = 'fixed';
stickyClose.style.display = 'unset';
if (rect.y < 0) {
resetPosition();
} else {
window.addEventListener('scroll', resetPosition, {once: true});
}
function resetPosition() {
setTimeout(function () {
stickyDiv.style.position = 'static';
}, timeout);
stickyClose.style.display = 'unset';
}
}
return function() {
$('#stickyClose').on('click', function() {
$('#billboardYa').remove();
});
var bind = setInterval(bindSticky, 1000);
setTimeout(function() {
clearInterval(bind);
}, 10000);
};
}

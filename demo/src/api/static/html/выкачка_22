function check_feedback (val) {
	if (val.text.value.length > 0) {
	send_feedback (val);
	}
	else {
		if (val.text.value.length==0) {
		alert("ÐÐ²ÐµÐ´Ð¸ÑÐµ Ð²Ð°ÑÐµ ÑÐ¾Ð¾Ð±ÑÐµÐ½Ð¸Ðµ!");
		document.feedback.text.focus();
		}
	}
}

function send_feedback (val) {
$.ajax({
url: "/feedback_sub.php",
type: "POST",
data: {name:val.name.value, text:val.text.value},
beforeSend: function(xhr){
$('#loading_feedback').css({'display':'block'});
},
success:function(data){
	if (data.match(/form_send/)) {
	$('#loading_text').html(data);
	}
}
});
}
</script>


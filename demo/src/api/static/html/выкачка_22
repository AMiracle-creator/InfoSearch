<script>

function show_count_like (json) {
try {
JSON.parse(json);
} catch (e) {
$('.like-count').hide()
return false;
}
var like_array = $.parseJSON(json);
$.each(like_array, function(index, value){
$('#like-'+index).text(value);
});
}

$('.like-today').click(function() {
var like = $(this).attr("id");
$.ajax({
  url: '/like.php?i='+like,
  cache: false,
  success: function(json){
	$('#'+like).hide();
	show_count_like(json);
  }
});
});

</script>


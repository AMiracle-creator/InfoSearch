<script>
$(document).ready(function(){
	$(window).scroll(function(){	
		if ($(window).scrollTop()>240){
			if ($('.menu_top').css('display') == 'none') {
			$('.menu_top').slideDown();
			$('#abs_left').animate({top: '60px'});
			}
		}
		else {
			if ($('.menu_top').css('display') == 'block') {
			$('.menu_top').slideUp();
			$('#abs_left').animate({top: '10px'});
			}
		}
	});
});


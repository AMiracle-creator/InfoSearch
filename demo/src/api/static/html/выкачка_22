function CheckDay(){
$.ajax({
url: '/checkday.php',
cache: false,
success: function(data){
	if (data.length>0&&data>0&&data!=20){
	window.location.reload(true);
	}
}
});
};

$(document).ready(function(){
setInterval(CheckDay,7200000);
});

var prazdnik_link = '';
$('.prazdnik_info').click(function() {
window.prazdnik_link = 'https://kakoysegodnyaprazdnik.ru'+$(this).attr('href');
function Link() {
var body_element = document.getElementsByTagName('body')[0];
var selection;
selection = window.getSelection();
var newdiv = document.createElement('div');
newdiv.style.position='absolute';
newdiv.style.left='-99999px';
body_element.appendChild(newdiv);
newdiv.innerHTML = window.prazdnik_link;
selection.selectAllChildren(newdiv);
window.setTimeout(function() {
body_element.removeChild(newdiv);
},0);
}
document.oncopy = Link;
});

</script>
</body>
</html>
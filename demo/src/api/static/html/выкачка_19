<script type="text/javascript">
	$(function () {
		var slider = $('.main-page-block');
		var slid = slider.find('.slider');
		var items = slider.find('.slider-items');
		slider.find('.slider-left').click(function () {
			if (!items.data('active')) {
				items.data('active', true);
				items.prepend(items.find('.slider-item:last').remove());
				items.css({ left: '-' + slid.innerWidth() + 'px' });
				items.animate({ left: 0 }, 300, function () {
					items.data('active', false);
				});
			}
		});
		slider.find('.slider-right').click(function () {
			if (!items.data('active')) {
				items.data('active', true);
				items.animate({ left: '-' + slid.innerWidth() + 'px' }, 300, function () {
					items.append(items.find('.slider-item:first').remove());
					items.css({ left: 0 });
					items.data('active', false);
				});
			}
		});
	});
</script>


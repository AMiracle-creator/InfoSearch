			}
											
			function hidepopup() {
				installcookie("whab",1);
				document.getElementById("fullscreenmf").remove();
				//document.getElementById("fullscreenmf").style.display = "none";
			}
							
			function openlink(URL) {
				installcookie("whab",1);
				document.getElementById("fullscreenmf").remove();
				//document.getElementById("fullscreenmf").style.display = "none";
				if (typeof URL !== 'undefined') {
					window.open(URL, '_blank');
				}
			}


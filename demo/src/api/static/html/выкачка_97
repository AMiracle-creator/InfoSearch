            // });


            var top = $('#adfox_side1').offset().top + 5;
            $(window).scroll(function (event) {
                // return;
                var y = $(this).scrollTop();


                if (y >= window.left_ad_height) {

                    // console.log(y, ' ', 345);
                    $('#adfox_side1').css({
                        'position': ' fixed',
                        'right': ' unset',
                        'top': ' 5px',
                        'left': ' unset',
                        // 'margin-left':' -240px'
                    });
                }
                else {
                    $('#adfox_side1').css({
                        'position': ' absolute',
                        'right': ' 0',
                        'top': window.left_ad_height + 'px',
                        'left': ' unset',
                        'margin-left': ' unset'
                    });
                }
                $('#adfox_side1').width($('#sticky-scroll-box').parent().width());
            });


            setTimeout(function () {
                $('.after-right').css({
                    'top': $('#side3').height() + 55 + 'px'
                });
                // $('.after-right-2').css({
                //         'top': $('#side3').height() + 25 + 'px'
                // });
            }, 1000);

            $(window).scroll(function (event) {


                // return;
                var y = $(this).scrollTop();
                if (y >= window.needTop - 20) {
                    $('#side3').css({
                        'position': ' fixed',
                        'top': '20px'
                    });
                    $('#crt-1316941').css({
                        'position': ' fixed',
                        'top': '20px'

     "query-input": "required name=search_term_string"
   }
 }





























    </script>
    
    
    <script>
        let vRefreshId = null;
        let autoRefresh = 1;
        window.addEventListener("loadFullJS", function () {
            vRefreshId = setInterval("window.location.reload()", 300000); //5 min

            document.addEventListener("touchstart", function () {
                if (autoRefresh === 1) {
                    autoRefresh = 0;
                    clearInterval(vRefreshId);
                }
            }, true);
            $('body').hover(function () {
                if (autoRefresh === 1) {
                    autoRefresh = 0;
                    clearInterval(vRefreshId);
                }
            });
            $(window).on("blur focus", function (e) {
                let prevType = $(this).data("prevType");
                let timerId;
                if (prevType !== e.type) {   //  reduce double fire issues
                    switch (e.type) {
                        case "blur":
                            timerId = setInterval(function () {
                                location.reload();
                            }, 300000);
                            break;
                        case "focus":
                            clearInterval(timerId);
                            break;
                    }
                }
                $(this).data("prevType", e.type);
            });
        });
    </script>
    
    <script>
        window.addEventListener("loadFullJS", function () {
            var hWrp = $('.blockA3new').height();
            if (hWrp === 0) {
                var size = 'auto';
            } else {
                size = hWrp - 100;
            }
            // if (window.matchMedia('(max-width: 1200px) and (min-width: 992px)').matches) {
            //     size = hWrp - 100;
            // }
            $('.lenta_limit').css({
                height: size
            });
            $(window).resize(function () {
                var hWrp = $('.blockA3new').height();
                if (hWrp === 0) {
                    var size = 'auto';
                } else {
                    size = hWrp - 100;
                }
                // if (window.matchMedia('(max-width: 1200px) and (min-width: 992px)').matches) {
                //     size = hWrp - 800;
                // }
                $('.lenta_limit').css({
                    height: size
                });
            });
            var delay = (function () {
                var timer = 300;
                return function (callback, ms) {
                    clearTimeout(timer);
                    timer = setTimeout(callback, ms);
                };
            })();
            news_page = 3;
            $('.show-hidden-news').click(function () {
                var hidden_news = $('.hidden-news');
                hidden_news.slideToggle('slow');
                hidden_news.removeClass('hidden-news');
                var post_data = {
                    news_page: news_page,
                    _method: 'POST'
                };
                delay(function () {
                    $.ajax({
                        data: post_data,
                        dataType: 'json',
                        headers: {
                            'X-CSRF-TOKEN': 'VqYcykWaLcmQzFhG9zrsbGxUTJmqMOKoZ4LwNXh1',
                        },
                        type: 'POST',
                        url: 'https://moe-online.ru/add_news',
                        success: function (response) {
                            if (response.end_paginate == 0) {
                                $('.show-hidden-news').remove();
                                $('.grad_w').remove();
                                console.log('нет больше');
                            }
                            $('.news-row').append(response['news']);
                            news_page = response['news_page'];
                        },
                        error: function (response) {
                        },
                    });
                }, 2000);
            });
            $(document).ready(function () {
                $('#comment_slider').slick({
                    slidesToShow: 3,
                    slidesToScroll: 1,
                    prevArrow: '<i class="fa fa-angle-left"></i>',
                    nextArrow: '<i class="fa fa-angle-right"></i>',
                    responsive: [
                        {
                            breakpoint: 991,
                            settings: {
                                slidesToShow: 2,
                                slidesToScroll: 1,
                            }
                        }]
                });
            });

            $('.h3-audio').delay(300).fadeIn(400);
            $('.audio-puls').delay(700).fadeIn(500);
        });
    </script>
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    

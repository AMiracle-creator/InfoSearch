                <script src="https://moe-online.ru/themes/default/front/js/probaclick/dist/probaclick.umd.js"></script>
    <script>
        let hasBeen = {
            prefetch: [],
            prerender: [],
        };
        const makeHint = (href, type) => {
            let link = document.createElement("link");
            link.setAttribute("rel", type);
            link.setAttribute("href", href);
            document.head.appendChild(link);
            return href;
        };
        const isExternalLink = (href) => {
            if (href !== null) {
                return !href.match(/^\//) && !href.includes(window.location.host);
            }
            return true;
        };
        const maybeMakeHint = ({link, type, max = null} = {}) => {
            let href = link.getAttribute("href");

            if (isExternalLink(href)) return;
            if (hasBeen[type].includes(href)) return;
            if (max !== null && hasBeen[type].length >= max) return;

            hasBeen[type].push(makeHint(href, type));
        };
        ProbaClick("a", {
            callback: function (link) {
                maybeMakeHint({
                    link,
                    type: "prefetch",
                });
            },
        });
        ProbaClick("a", {
            delay: 1000,
            count: 3,
            callback: function (link) {
                maybeMakeHint({
                    link,
                    type: "prerender",
                    max: 1,
                });
            },
        });

        function dynamicLoad(src, callback) {
            let script = document.createElement("script");
            script.type = "text/javascript";
            script.src = src;
            script.async = 'async';
            script.onload = () => callback(script);
            document.head.appendChild(script);
        }

        window.addEventListener("loadFullJS", function () {

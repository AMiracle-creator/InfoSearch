    }());

    var Messaging = (function () {
        function Messaging(__app) {
            var _this = this;
            var __service_worker = null;
            this.ru_primamedia_Messaging__messaging = __app.messaging();
            this.ru_primamedia_Messaging__messaging.onTokenRefresh(function () {
                _this.ru_primamedia_Messaging__function_token_action(_this.onSubscribe);
            });

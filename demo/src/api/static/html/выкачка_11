    var MessagingController = (function () {
        function MessagingController(ru_primamedia_MessagingController__number_region_id, ru_primamedia_MessagingController__messaging, __string_subscribe_selector, __string_unsubscribe_selector, __string_subscribe_icon, __string_unsubscribe_icon) {
            var _this = this;
            this.ru_primamedia_MessagingController__number_region_id = ru_primamedia_MessagingController__number_region_id;
            this.ru_primamedia_MessagingController__messaging = ru_primamedia_MessagingController__messaging;
            this.ru_primamedia_MessagingController__messaging.onSubscribe = function (__string_token) {
                $.getJSON("/push/subscribe/", {
                    PushSubscription: {
                        token: __string_token,
                        id_region: _this.ru_primamedia_MessagingController__number_region_id
                    }
                }).done(function (__any_response) {
                    if (__any_response)
                        localStorage.setItem("web-push", "!");
                    else
                        alert("Ошибка! Не удалось оформить подписку.");
                }).fail(function () {
                    alert("Ошибка! Не удалось оформить подписку. Попробуйте еще раз.");
                });
                _this.ru_primamedia_MessagingController__function_toggle(_this.ru_primamedia_MessagingController__j_query_subscribe, _this.ru_primamedia_MessagingController__j_query_unsubscribe);
            };
            this.ru_primamedia_MessagingController__messaging.onUnsubscribe = function (__string_token) {
                $.getJSON("/push/unsubscribe/", {
                    token: __string_token
                }).done(function (__any_response) {
                    localStorage.removeItem("web-push");
                });
                _this.ru_primamedia_MessagingController__function_toggle(_this.ru_primamedia_MessagingController__j_query_unsubscribe, _this.ru_primamedia_MessagingController__j_query_subscribe);
            };
            this.ru_primamedia_MessagingController__j_query_subscribe = $(__string_subscribe_selector).append("<img src=" + __string_subscribe_icon + " />").click(function (__click_event) {
                __click_event.preventDefault();
                _this.ru_primamedia_MessagingController__messaging.subscribe();
                _this.ru_primamedia_MessagingController__function_block(_this.ru_primamedia_MessagingController__j_query_subscribe);
            }).hide();
            this.ru_primamedia_MessagingController__j_query_unsubscribe = $(__string_unsubscribe_selector).append("<img src=" + __string_unsubscribe_icon + " />").click(function (__click_event) {
                __click_event.preventDefault();

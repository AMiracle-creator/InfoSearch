            __j_query_element.css("pointer-events", "none").animate({ opacity: 0 }, 450);
        };
        MessagingController.prototype.ru_primamedia_MessagingController__function_toggle = function (__j_query_element_1, __j_query_element_2) {
            __j_query_element_1.hide(), __j_query_element_2.show().css("opacity", 1);
        };

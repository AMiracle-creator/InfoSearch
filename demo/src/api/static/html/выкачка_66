<script>


function numberWithSpaces(x) {
    x = x.toString().replaceAll(" ", "");
    return x.replace(/\B(?=(\d{3})+(?!\d))/g, " ");
}

$(document).ready(function() {

    $('.search-price').each(function(i, mainBlock){

        let checked = false;

        let currencyBlocks = $(mainBlock).find('input[name*="currency"]');

        $.each(currencyBlocks, function(j, currencyBlock){
            if($(currencyBlock).is(':checked')){
                checked = true;
            }
        });

        if(!checked){
            currencyBlocks.first().attr('checked', 'checked');
        }

        /* нинициализация ползунка */
        $(mainBlock).find('.range-price').ionRangeSlider({
            onChange: function(data) {
                $(mainBlock).find('.price-from').val(numberWithSpaces(data.from));
                $(mainBlock).find('.price-to').val(numberWithSpaces(data.to));
            },
            skin: 'round',
            type: 'double',
            step: '500',
            postfix: ' ' + ($(mainBlock).find('input[name*="currency"]:checked').parent().text()).trim(),
            force_edges: true,
            min: 0,
            max: 1000000,
            from: 51500,
            to: 786000
        });

        /* нужен следить за постфиксом на балуне при смене поисковой валюты */
        $.each(currencyBlocks, function(j, currencyBlock){
            $(currencyBlock).change(function() {
                let my_range = $(mainBlock).find('.range-price').data("ionRangeSlider");
                my_range.update({
                    postfix: ' ' + ($(mainBlock).find('input[name*="currency"]:checked').parent().text()).trim(),
                });
            });
        });

        /* по событию ввода меняем занчение с добавлением форматирования и передвигаем палзунок */
        $('.price-from, .price-to').on('input', function () {

            let price = $(this).val().replace (/[^\+\d]/g, '');

            if($(this).hasClass('price-from')) {
                let priceRangeBlock = $('.range-price').data("ionRangeSlider");
                priceRangeBlock.update({
                    from: price,
                    to: $('.price-to').val().replace(/[^\+\d]/g, '')
                });
            } else if($(this).hasClass('price-to')){
                let priceRangeBlock = $('.range-price').data("ionRangeSlider");
                priceRangeBlock.update({
                    from: $('.price-from').val().replace(/[^\+\d]/g, ''),
                    to: price
                });
            }

            $(this).val(numberWithSpaces(price));

        });

    });

});

</script>


<script type="text/javascript">

    $(function(){

        $.getJSON("https://realt.by/typo3temp/uedb_menu/locations.json", function( locations ) {
            $('#selectRegionResidential.select2, #selectRegionCountry.select2, #selectRegionCommercial.select2').select2({
                placeholder:"Город либо регион поиска",
                data: locations,
                minimumInputLength: 2,
                allowClear: true,
                language: 'ru',
            });
        });

        $("#form").submit(function(e){
            $("div:hidden").each(function(){
                $(this).remove();
            });
            return true;
        });

        $('.common-search-submit').on('click', function(event) {
            dataLayer.push({'event': 'common-search-click-select'});
            console.log("dataLayer.push({'event': 'common-search-click-select'})");

            $(this).closest('form').submit();
        });

        $('.common-search-submit-map').on('click', function(event) {
            dataLayer.push({'event': 'common-search-click-map'});
            console.log("dataLayer.push({'event': 'common-search-click-map'})");

            $('.common-search-map-flag').val('1');
            $(this).closest('form').submit();
        });

        $('.common-search-submit-search').on('click', function(event) {
            /* 2020/12 not used
            dataLayer.push({'event': 'common-search-click-search'});
            console.log("dataLayer.push({'event': 'common-search-click-search'})");
            */

            $('.common-search-search-flag').val('1');
            $(this).closest('form').submit();
        });

    });


</script>



<script type="text/javascript">
    window.currencyRates = {"112":{"112":1,"643":0.002658373,"840":3.5559e-5,"933":0.0001,"978":3.3428e-5},"643":{"112":376.17,"643":1,"840":0.01337636,"933":0.037617,"978":0.012574628},"840":{"112":28122,"643":74.758752692,"840":1,"933":2.8122,"978":0.940063513},"933":{"112":10000,"643":26.583725443,"840":0.355593486,"933":1,"978":0.334280461},"978":{"112":29915,"643":79.525214664,"840":1.063757912,"933":2.9915,"978":1}};
</script><script type="text/javascript">

    //load banner for root slider banner
    //<div data-img="https://realt.by/fileadmin/realt.adaptive/images/main-filter/residential-1.jpg" data-red_ribbon="Скидка до 15%" data-title="ЖК «Minsk World»" data-desc="Праздничная акция на квартиры в самом центре Минска и подарки клиентам!" data-target="https://realt.by/"></div>
    //<div data-img="https://realt.by/fileadmin/realt.adaptive/images/main-filter/residential-2.jpg" data-red_ribbon="Паркинг в подарок" data-title="ЖК «Гранд Хаус»" data-desc="Специальное предложение на трехкомнатные квартиры" data-target="https://realt.by/"></div>
    //<div data-img="https://realt.by/fileadmin/realt.adaptive/images/main-filter/residential-3.jpg" data-red_ribbon="Рассрочка до 100 месяцев" data-title="ЖК «Минск Мир»" data-desc="В столичном комплексе старт продаж в трех новых домах" data-target="https://realt.by/"></div>

    $(document).ready(function () {

        window.defaultRootSloganBG = 'fileadmin/realt.adaptive/images/main-filter/background-foto.jpg';
        window.defaultRootSlogan = $('.main-filter .filter-slogan a').text();
        window.defaultRootSloganUrl = 'javascript:void(0);';

        let timerLoadDefaultImage;
        let banner_swipers = {};
        banner_swipers = {
            'initSwiperFilterResidential': {
                'className': 'swiper-filter-residential',
                'tab_name': 'residentialInputs',
                'zone': 864,
                exc_banners: null,
                'obj': null
            },
            'initSwiperFilterCountry': {
                'className': 'swiper-filter-country',
                'tab_name': 'countryInputs',
                'zone': 865,
                exc_banners: null,
                'obj': null
            },
            'initSwiperFilterCommercial': {
                'className': 'swiper-filter-commercial',
                'tab_name': 'commercialInputs',
                'zone': 866,
                exc_banners: null,
                'obj': null
            }
        };

        let render_swiper_item = function (containerClass = '', click_url = '', log_url = '', html_content = '') {

            let banner_params = $(html_content).data();
            if (!(banner_params.title && banner_params.img)) return '';

            let slogan = banner_params.h1 || window.defaultRootSlogan;
            let slogan_color = banner_params.black || 0;
            if (slogan_color === 0) {
                slogan_color = '#fff';
            } else {
                slogan_color = '#333';
            }
            console.log(banner_params);

            let slogan_url = (slogan !== window.defaultRootSlogan) ? click_url : window.defaultRootSloganUrl;
            let slogan_target = (slogan !== window.defaultRootSlogan) ? '_blank' : '';

            let template_item = '' +
                '<div class="swiper-slide" data-log_url="' + log_url + '" data-image="' + banner_params.img +
                '" data-h1="' + (banner_params.h1 || window.defaultRootSlogan) +
                '" data-slogan_color="' + slogan_color +
                '" data-slogan_url="' + slogan_url +
                '" data-slogan_target="' + slogan_target +
                '">' +
                    (banner_params.red_ribbon ? '<div class="ribbon mb-6">                                                        ' +
                        '<svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">' +
                            '<path d="M12.0001 21.9995C12.0001 16.8095 16.359 12.5395 21.9476 12.0495C22.0176 12.0395 22.0176 11.9595 21.9476 11.9495C16.359 11.4595 12.0001 7.18951 12.0001 1.99951C12.0001 7.18951 7.64121 11.4595 2.05261 11.9495C1.98263 11.9595 1.98263 12.0395 2.05261 12.0495C7.64121 12.5395 12.0001 16.8095 12.0001 21.9995Z" stroke="#FFD02F" stroke-width="2" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/>' +
                            '<path d="M3.1925 2.72021L6.19251 5.72021" stroke="#FFD02F" stroke-width="2" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/>' +
                            '<path d="M21.1884 2.71973L18.1884 5.71973" stroke="#FFD02F" stroke-width="2" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/>' +
                            '<path d="M3.1925 20.7202L6.19251 17.7202" stroke="#FFD02F" stroke-width="2" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/>' +
                            '<path d="M21.1884 20.7197L18.1884 17.7197" stroke="#FFD02F" stroke-width="2" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/>' +
                        '</svg>' +
                        '<span>' + banner_params.red_ribbon + '</span></div>' : '') +
                    '<div class="fs-large">' + banner_params.title + '</div>' +
                    (banner_params.desc ? '<div class="fs-small mb-10">' + banner_params.desc + '</div>' : '') +
                    '<div class="media align-items-center">' +
                        '<a href="' + click_url + '" class="btn btn-light btn-sm" target="_blank">Подробнее</a>' +
                        '<div class="media-body">' +
                            '<div class="swiper-pagination"></div>' +
                        '</div>' +
                    '</div>' +
                '</div>';

            $("." + containerClass + " .swiper-wrapper").append(template_item);
        }

        function get_swiper_banners(index = null, iter = 0) {

            if (banner_swipers[index].zone === undefined || banner_swipers[index].zone === null) return null;

            if (iter >= 5) {
                init_swiper(index);
                return null;
            }

            let banner_id = null;
            let banner_html = null;
            let click_url = null;
            let log_url = null;
            let adv_url = 'https://a.realt.by/www/delivery/ax.php?zoneid=' + banner_swipers[index].zone;

            if (banner_swipers[index].exc_banners instanceof Array) {
                adv_url = adv_url + '&exclude=' + banner_swipers[index].exc_banners.join(',');
            }

            var request = new XMLHttpRequest();
            request.open('GET', adv_url, true);
            request.onload = function () {
                if (this.status === 200) {
                    if (window.DOMParser) {
                        parser = new DOMParser();
                        xmlDoc = parser.parseFromString(this.responseText, "text/xml");
                    } else {
                        xmlDoc = new ActiveXObject("Microsoft.XMLDOM");
                        xmlDoc.async = false;
                        xmlDoc.loadXML(this.responseText);
                    }

                    if (xmlDoc.getElementsByTagName("bannerid").length) {
                        banner_id = xmlDoc.getElementsByTagName("bannerid")[0].childNodes[0].nodeValue;
                        banner_html = xmlDoc.getElementsByTagName("htmltemplate")[0].childNodes[0].nodeValue;

                        //Нужно сделать загрузку баннеров до готоыности страницы.
                        // клики
                        click_url = xmlDoc.getElementsByTagName("clickUrl")[0].childNodes[0].nodeValue;
                        // показы
                        log_url = xmlDoc.getElementsByTagName("logUrl")[0].childNodes[0].nodeValue;

                        iter = iter + 1;
                        render_swiper_item(banner_swipers[index].className, click_url, log_url, banner_html);
                        if (banner_swipers[index].exc_banners === null) banner_swipers[index].exc_banners = [];
                        banner_swipers[index].exc_banners.push(banner_id);
                        get_swiper_banners(index, iter);

                    } else if (iter > 0) {
                        init_swiper(index);
                    }
                }
            };
            request.send();

        }

        function init_swiper(index) {
            banner_swipers[index].obj = new Swiper('.' + banner_swipers[index].className, {
                autoplay: {
                    delay: 5000
                },
                watchOverflow: true,
                initialSlide: 0,
                effect: 'fade',
                fadeEffect: {
                    crossFade: true
                },
                pagination: {
                    el: '.swiper-pagination',
                    clickable: true
                },
                on: {
                    slideChange: function () {
                        let image = $(this.slides[this.activeIndex]).data('image');
                        let log_url = $(this.slides[this.activeIndex]).data('log_url');
                        let slogan_color = $(this.slides[this.activeIndex]).data('slogan_color');
                        let slogan_url = $(this.slides[this.activeIndex]).data('slogan_url');
                        let slogan_target = $(this.slides[this.activeIndex]).data('slogan_target');
                        let h1 = $(this.slides[this.activeIndex]).data('h1');

                        if (image) {
                            $('.main-filter').css('background-image', 'url(' + image + ')');
                            $('.main-filter .filter-slogan a')
                                .attr('href', slogan_url)
                                .attr('target', slogan_target)
                                .css('color', slogan_color)
                                .text(h1);
                            $('.main-filter a.main-filter-link')
                                .attr('href', slogan_url)
                                .attr('target', slogan_target);
                        }

                        if (log_url) {
                            // надо просто дернуть по http, что бы показ был логирован
                            $.get(log_url, function (data) {});
                            // делаем это один раз!
                            $(this.slides[this.activeIndex]).attr('data-log_url', '')
                        }
                    },
                    init: function () {
                        let log_url = $(this.slides[this.activeIndex]).attr('data-log_url');
                        if (log_url) {
                            $.get(log_url, function (data) {});
                            //log first time and ignore others
                            $(this.slides[this.activeIndex]).attr('data-log_url', '');
                        }
                    },
                }
            });

            if ($('.' + banner_swipers[index].className).length && $('#' + banner_swipers[index].tab_name).hasClass('active')) {
                let slide = banner_swipers[index].obj.slides[banner_swipers[index].obj.activeIndex];
                let image = $(slide).data('image');
                let slogan_color = $(slide).data('slogan_color');
                let slogan_url = $(slide).data('slogan_url');
                let slogan_target = $(slide).data('slogan_target');
                let h1 = $(slide).data('h1');
                if (image) {
                    $('.main-filter').css('background-image', 'url(' + image + ')');
                    $('.main-filter .filter-slogan a')
                        .attr('href', slogan_url)
                        .attr('target', slogan_target)
                        .css('color', slogan_color)
                        .text(h1);
                    $('.main-filter a.main-filter-link')
                        .attr('href', slogan_url)
                        .attr('target', slogan_target);
                }
                banner_swipers[index].obj.autoplay.start();
            }

        }

        function setLoaderImage(time) {
            //Даем время на закрузку чего-то кроме картинки по умолчанию
            timerLoadDefaultImage = setTimeout(function () {
                $('.main-filter').css('background-image', 'url("' + window.defaultRootSloganBG + '")');
                $('.main-filter .filter-slogan a').attr('href', window.defaultRootSloganUrl).attr('target', '').text(window.defaultRootSlogan);
                $('.main-filter a.main-filter-link').attr('href', window.defaultRootSloganUrl).attr('target', '');
            }, time);
        }

        function clearLoaderImage() {
            // Не загружаем картинку по умолчанию
            if (timerLoadDefaultImage) {
                clearTimeout(timerLoadDefaultImage);
                timerLoadDefaultImage = null;
            }
        }


        //start
        $.each(banner_swipers, function (index) {

            get_swiper_banners(index);

            $('.main-filter .filter-form .navi a[href="#' + banner_swipers[index].tab_name + '"]').click(function (event) {

                event.preventDefault();

                //load banner image or default
                if (banner_swipers[index].obj !== null) {
                    banner_swipers[index].obj.update();
                    let slide = banner_swipers[index].obj.slides[banner_swipers[index].obj.activeIndex];
                    let image = $(slide).data('image');
                    let slogan_color = $(slide).data('slogan_color');
                    let slogan_url = $(slide).data('slogan_url');
                    let slogan_target = $(slide).data('slogan_target');
                    let h1 = $(slide).data('h1');
                    if (image) {
                        clearLoaderImage();
                        $('.main-filter').css('background-image', 'url(' + image + ')');
                        $('.main-filter .filter-slogan a')
                            .attr('href', slogan_url)
                            .attr('target', slogan_target)
                            .css('color', slogan_color)
                            .text(h1);
                        $('.main-filter a.main-filter-link')
                            .attr('href', slogan_url)
                            .attr('target', slogan_target);
                    }
                } else {
                    setLoaderImage(500);
                }

                //stopall sliders
                $.each(banner_swipers, function (name) {
                    if (banner_swipers[name].obj !== null) {
                        banner_swipers[name].obj.autoplay.stop();
                    }
                });

                //start current
                if (banner_swipers[index].obj !== null) {
                    banner_swipers[index].obj.autoplay.start();
                }

            });

        });

    });
</script>

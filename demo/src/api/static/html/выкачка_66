<script>
    
        function sendStatVote(element){

            let adv_url = '?eID=tx_uedbstatistics_votes&action=set-vote&vote=' + element.textContent.trim();

            var request = new XMLHttpRequest();
            request.open('GET', adv_url, true);
            request.onload = function() {
                if (this.status == 200) {

                    let data = JSON.parse(this.responseText);

                    if(data.status == 200){

                        let items = '<div class="progress mb-6" style="height:20px; width:100%;">';

                        for(let i = 0; i < data.records.votes.length; i++){
                            let vote = data.records.votes[i];
                            items += '<div class="progress-bar ' + (vote.vote == 'Снизятся' ? 'bg-danger' : 'bg-success') + '" role="progressbar" title="' + vote.vote + '" data-toggle="tooltip" style="width: ' + vote.percent + '%" aria-valuenow="' + vote.cnt_vote + '" aria-valuemin="0" aria-valuemax="' + data.records.max_votes + '">' +
                                            Math.round(vote.percent) + '%' +
                                      '</div>';
                        }

                        items += '</div>';

                        element.parentNode.nextElementSibling.textContent = "(голоса пользователей)";
                        element.parentNode.innerHTML = items;


                    } else if(data.status == 400 && data.text == 'user is not authorized'){
                        showLoginBox();
                    } else {
                        element.parentNode.nextElementSibling.textContent = "";
                        element.parentNode.innerHTML = 'Произошла ошибка, мы работаем на ее устранением!';
                    }

                }
            };
            request.send();

        }
    
</script>

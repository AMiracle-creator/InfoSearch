<script>
    window.addEventListener('load', function () {
        // console.log('maintrands loaded');
        let mtrends = document.getElementById('index-maintrend').querySelectorAll('a');
        let wtotal = 0;
        if (mtrends) {
            for (let z = 0; z < mtrends.length; ++z) {
                const element = mtrends[z];
                let w = window.getComputedStyle(element);
                wtotal = wtotal + parseFloat(w.width);
            }
            let pd = (960 - wtotal) / 12;
            for (let z = 0; z < mtrends.length; ++z) {
                const element = mtrends[z];
                element.style.paddingLeft = pd + 'px';
                element.style.paddingRight = pd + 'px';
            }
        }
    })
</script>


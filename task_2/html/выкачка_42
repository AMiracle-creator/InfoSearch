        $(document).ready(function () {
            $('.news_list_main_js').aifNewsList({
                url: '/news/index/all/tpl/index%2Flast_news.phtml'
            });
            var mainNewsTabsEl = $('.main_news_list_tabs_js');
            var onTabOpen = function (tab) {
                tab.content_container.find('.list_js').scrollTop(0);
            };
            if (mainNewsTabsEl.length) {
                mainNewsTabsEl.aiftabs(
                {
                    tabs : [
                        {
                            onOpen: function () {
                                onTabOpen(this);
                            }
                        },
                        {
                            onOpen: function () {
                                onTabOpen(this);
                            }
                        }
                    ]
                }
                );
            }
            var regionNewsList = $('.news_list_main_region_js');
            if (regionNewsList.length) {
                regionNewsList.aifNewsList({
                    url: '/news/index/all/tpl/index%2Flast_news.phtml/current_region/1'
                });
            }
        });
    </script>
</div>

            

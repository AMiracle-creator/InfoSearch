<script>jQuery(function ($) {

    var $suggestInput = $(".search__textbox");
    var showEventSent = false;
    var autoComSelector = ".autocom";
    var autoComListSelector = ".autocom__list";
    var autoComItemSelector = ".autocom__item";
    $suggestInput.on("input", function () {
        var autocomList = $suggestInput.closest(autoComSelector).find(autoComListSelector);
        var intervalOnShow = setInterval(function () {
            if (autocomList.length === 0) {
                autocomList = $suggestInput.closest(autoComSelector).find(autoComListSelector);
            } else {
                if (!showEventSent) {
                    showEventSent = true;
                    try {
                        ga("send", "event", "HelpTags", "show");
                    } catch(err) {}
                }
                clearInterval(intervalOnShow);
                autocomList = $suggestInput.closest(autoComSelector).find(autoComListSelector);
                var autocomItem = autocomList.find(autoComItemSelector);
                autocomItem.off("click").on("click", function (e) {
                    var value = $(this).text();
                    try {
                        ga("send", "event", "HelpTags", "click", value);
                    } catch(err) {}
                });
            }
        }, 200);
    });


    $('.search-form__advanced-button').on('click', function(e) {
        var value = $('.search-tabs__tab.search-tabs__tab_active').data('value');
        var linkParam = '';
        if (value.length > 1) {
            if (value === '/resumes') {
                linkParam = 'resume';
            } else {
                if (value === '/companies') {
                    linkParam = 'company';
                }
            }
            this.setAttribute('href', this.getAttribute('href') + '?searchType=' + linkParam);
        }
    });


    try {
        ga("send", "event", "mainPageTags", "show");
    } catch (err) {}

    $(document).on("click", "a.ym-vts", function() {
        try {
            ga("send", "event", "mainPageTags", "click", this.text);
        } catch (err) {}
    });


try {
    $('[data-event="1"]').on('click', function(event) {
        try {
            var element = event.currentTarget;
            var category = element.dataset['event_category'];
            var action = element.dataset['event_action'];
            var label = element.dataset['event_label'];
            ga('send', 'event', category, action, label);
        } catch (err) {}
    });
} catch (err) {}


    $('.search__action-list .action-list__list .action-list__item').on('click', function(e) {
        var value = this.dataset['value'];
        if (value.includes('/resumes')) {
           url = 'https://gorodrabot.ru/advanced_search?searchType=resume';
        } else if (value.includes('/companies')) {
           url = 'https://gorodrabot.ru/advanced_search?searchType=company';
        } else {
           url = 'https://gorodrabot.ru/advanced_search';
        }
        
        $('.header__advanced-button').attr('href', url);
    });

});</script></body>

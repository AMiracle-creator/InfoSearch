<script type="text/javascript">
		jQuery('.togglesearch').click(function() {
		//console.log('search_form');
		//из-за 2 кнопок открытия меню toggle не подходит
		//проверить открыто ли меню
		if ( jQuery('.menu_popup').hasClass('open') ) {
			//удалить классы
			jQuery('.menu_popup').removeClass('open');
			jQuery('.up_plash').removeClass('gray_menu');
			//jQuery('body').removeClass('no-scroll');
		} else 
		{
			jQuery('.menu_popup').addClass('open');
			jQuery('.up_plash').addClass('gray_menu');
		//	jQuery('body').addClass('no-scroll');
		}
		if ( jQuery('.menu_popup').hasClass('open') ) {
			//проверяем изменения, если открыто, доюавляем скролл
			jQuery('#find_text').focus();
			jQuery('.menu_popup').jScrollPane({showArrows: true, animateScroll: true, animateDuration: 500, arrowScrollOnHover: true, maintainPosition: false});
		}
	});
	jQuery('#btn_menu_popup').click(function(){
			if ( jQuery('.menu_popup').hasClass('open') ) {
			//удалить классы
			jQuery('.menu_popup').removeClass('open'); 
			jQuery('.up_plash').removeClass('gray_menu');
			//jQuery('body').removeClass('no-scroll');
		} else 
		{
			jQuery('.menu_popup').addClass('open');
			jQuery('.up_plash').addClass('gray_menu');
		//	jQuery('body').addClass('no-scroll');
		}
		if ( jQuery('.menu_popup').hasClass('open') ) {
			//проверяем изменения, если открыто, доюавляем скролл
			jQuery('#find_text').focus();
			jQuery('.menu_popup').jScrollPane({showArrows: true, animateScroll: true, animateDuration: 500, arrowScrollOnHover: true, maintainPosition: false});
		}
		});

jQuery(document).click(function (e) {	
const menuBtn = jQuery('#btn_menu_popup'),
	  menuBtnMob = jQuery('.mobilemenu-toggle i'),
      menu    = jQuery('.menu_popup');	
	//if ( !menuBtn.is(e.target) && !menu.is(e.target) && menu.has(e.target).length === 0) {

if ( !menuBtn.is(e.target) && !menuBtnMob.is(e.target) && !(jQuery('#btn_menu_popup i')).is(e.target) && !(jQuery('.togglesearch i')).is(e.target) && !(jQuery('#find_text')).is(e.target)) {
	  jQuery('.menu_popup').removeClass('open'); 
	  jQuery('.up_plash').removeClass('gray_menu');
    };
});
		//Клик по мобильному меню
	jQuery(".mobilemenu-toggle").click(function() {

	//	jQuery('body').toggleClass('no-scroll');
	//	jQuery(this).toggleClass('open');
	//	jQuery('.mobilemenu').toggleClass('open');
	if ( jQuery('.menu_popup').hasClass('open') ) {
			//удалить классы
			jQuery('.menu_popup').removeClass('open');
			jQuery('.up_plash').removeClass('gray_menu');			
		} else 	{
			jQuery('.menu_popup').addClass('open');
			jQuery('.up_plash').addClass('gray_menu');
		}
